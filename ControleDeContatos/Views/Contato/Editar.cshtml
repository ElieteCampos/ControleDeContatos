@{
    ViewData["Title"] = "Editar Contatos";
}
<div class="text-center">


    <h1 class="display-4">Editar Contatos</h1>


</div>
<form>
    <div class="form-group mb-3">
        <label for="nome" class="form-label">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">

    </div>

    <div class="form-group mb-3">
        <label for="email" class="form-label">Email</label>
        <input type="email" class="form-control" placeholder="Digite o email">

    </div>
    <div class="form-group mb-3">
        <label for="celular" class="form-label">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o celular do contato">

    </div>

    <button type="submit" class="btn btn-primary">Alterar</button>
    <a class="btn btn-secondary" role="button" asp-controller="Contato" asp-action="Index">Voltar</a>
</form>